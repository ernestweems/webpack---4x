  "bugs": {
    "url": "https://github.com/ernestweems/webpack---4x/issues"
  },
  "homepage": "https://github.com/ernestweems/webpack---4x#readme",
  "devDependencies": {
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  }
}
