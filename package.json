{
  "name": "webpack-4x",
  "version": "1.0.0",
  "description": "base webpack configuration for webpack version 4",
  "main": "index.js",
  "scripts": {
    "webpack":"webpack",
    "debug":"node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev":"npm run webpack -- --mode development",
    "watch":"npm run webpack --  --mode development --watch",
    "prod":"npm run webpack --  --mode production",
    "dev:debug":"npm run debug -- --mode development",
    "prod:debug":"npm run debug -- --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ernestweems/webpack---4x.git"
  },
